<?xml version="1.0" encoding="utf-8"?>
<project>
	<!-- NMML reference: https://gist.github.com/1763850 -->
	
	<!-- metadata, make sure 'package' is at least 3 segments (ie. com.mycompany.myproject) -->
	<meta title="FlappyObamas" package="FlappyObamas" version="1.0.0" company="sdaf" />
	
	<!-- output -->
	<app main="Main" file="FlappyObamas" path="bin" />
	
	<window background="#FFFFFF" fps="60" />
	<window width="800" height="480" unless="mobile" />
	<window orientation="landscape" vsync="false" antialiasing="0" if="cpp" />
	
	<!-- classpath, haxe libs -->
	<source path="src" />
	<haxelib name="openfl" />
	<haxelib name="actuate" />
	
	<!-- assets -->
	<icon path="assets/openfl.svg" />
	<assets path="assets/Extrude.ttf" id="gamefont" glyphs="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz '-&amp;quot;'%,?!$+:*.éèêëàäâïîüç0123456789+=&amp;gt;&amp;lt;()[]/âàäéèêëîïôöùûüçáéíóúÁÉÍÓÚÑñ¡¿äÄÖÜß_²αβ" />
	<assets path="assets/img" rename="img" />
	<assets path="assets/sfx" rename="sfx" />
	
	<!-- optimize output
	<haxeflag name="-dce full" /> -->
	
</project>